{
    "manifest_version": 3,
   
    "name": "chess++",
    "version": "1.0.0",
    "description": "more chess.com stats",

    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["activeTab"],
    "content_scripts": [{
      "js": ["index.js"],
      "css": ["style.css"],
      "matches": ["https://www.chess.com/*"]
    }],
    "author": "KD3n1z",
    "action": {
        "default_title": "chess++",
        "default_icon": {
          "32": "icon32.png"
        }
    }
}